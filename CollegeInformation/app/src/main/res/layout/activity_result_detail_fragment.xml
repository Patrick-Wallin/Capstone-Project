                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@id/admission_sat_reading_25_label_text_view"
                            android:gravity="right"/>

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="10dp">

                        <TextView
                            android:id="@+id/admission_sat_reading_75_label_text_view"
                            android:text="@string/label_sat_critical_reading_75"
                            android:textColor="@color/colorPrimary"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true" />

                        <TextView
                            android:id="@+id/admission_sat_reading_75_value_text_view"
                            android:text="@string/default_value_of_number_on_college"
                            android:textColor="@color/colorPrimary"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@id/admission_sat_reading_75_label_text_view"
                            android:gravity="right"/>

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
