<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/draw_insets_frame_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.design.widget.AppBarLayout
        android:id="@+id/app_bar"
        android:elevation="@dimen/appbar_elevation"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        Â <android.support.design.widget.CollapsingToolbarLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_scrollFlags="scroll|exitUntilCollapsed">

            <ImageView
                android:id="@+id/college_image_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scaleType="centerCrop"
                android:adjustViewBounds="true"
                app:layout_collapseMode="parallax"
                app:layout_collapseParallaxMultiplier="0.7"
                tools:ignore="ContentDescription"
                />

            <android.support.v7.widget.Toolbar
                android:id="@+id/detail_toolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_collapseMode="pin"
                android:contentInsetStart="0dp"
                android:contentInsetLeft="0dp"
                app:contentInsetLeft="0dp"
                app:contentInsetStart="0dp"
                android:minHeight="0dp"
                android:paddingLeft="0dp"
                android:paddingStart="0dp"
                android:paddingRight="0dp"
                android:paddingEnd="0dp"
                app:layout_scrollFlags="scroll|enterAlways"
                android:layout_gravity="bottom">

                <LinearLayout
                    android:orientation="vertical"
                    android:elevation="6dp"
                    android:background="@color/background_on_title_and_subtitle"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="10dp">

                        <TextView
                            android:id="@+id/college_name_text_view"
                            android:textColor="@android:color/white"
                            android:textStyle="bold"
                            android:textAppearance="?android:textAppearanceMedium"
                            android:text="@string/label_website"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left"
                            android:gravity="left"
                            android:layout_alignParentLeft="true" />

                        <ImageView
                            android:id="@+id/likes_image_view"
                            android:src="@drawable/ic_thumb_up_black_24dp"
                            android:tag="@string/not_favorite_college"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:layout_alignParentRight="true"
                            android:contentDescription="@string/content_desc_likes"
                            android:gravity="right"/>

                    </RelativeLayout>
                </LinearLayout>
            </android.support.v7.widget.Toolbar>
        </android.support.design.widget.CollapsingToolbarLayout>
    </android.support.design.widget.AppBarLayout>

    <android.support.v4.widget.NestedScrollView
        android:id="@+id/scrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp">

                    <TextView
                        android:id="@+id/website_label_text_view"
                        android:textColor="@color/colorPrimary"
                        android:text="@string/label_website"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true" />

                    <TextView
                        android:id="@+id/website_value_text_view"
                        android:textColor="@color/colorPrimary"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@id/website_label_text_view"
                        android:gravity="right"/>

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp">

                    <TextView
                        android:id="@+id/location_label_text_view"
                        android:textColor="@color/colorPrimary"
                        android:text="@string/label_location"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true" />

                    <TextView
                        android:id="@+id/location_value_text_view"
                        android:textColor="@color/colorPrimary"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@id/location_label_text_view"
                        android:gravity="right"/>

                </RelativeLayout>

                <View style="@style/Divider"/>

                <LinearLayout
                    android:orientation="vertical"
                    android:elevation="6dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="@string/label_cost"
                        android:textColor="@color/colorPrimary"
                        android:textStyle="bold"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="10dp">

                        <TextView
                            android:id="@+id/tuition_in_state_label_text_view"
                            android:textColor="@color/colorPrimary"
                            android:text="@string/label_tuition_in_state"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true" />

                        <TextView
                            android:id="@+id/tuition_in_state_value_text_view"
                            android:textColor="@color/colorPrimary"
                            android:text="@string/default_value_of_number_on_college"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@id/tuition_in_state_label_text_view"
                            android:gravity="right"/>

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="10dp">

                        <TextView
                            android:id="@+id/tuition_out_state_label_text_view"
                            android:textColor="@color/colorPrimary"
                            android:text="@string/label_tuition_out_state"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true" />

                        <TextView
                            android:id="@+id/tuition_out_state_value_text_view"
                            android:textColor="@color/colorPrimary"
                            android:text="@string/default_value_of_number_on_college"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@id/tuition_out_state_label_text_view"
                            android:gravity="right"/>

                    </RelativeLayout>
                </LinearLayout>


                <View style="@style/Divider"/>

                <LinearLayout
                    android:orientation="vertical"
                    android:elevation="6dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/colorPrimary"
                        android:textStyle="bold"
                        android:text="@string/label_aid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="10dp">

                        <TextView
                            android:id="@+id/aid_fed_loans_label_text_view"
                            android:textColor="@color/colorPrimary"
                            android:text="@string/label_undergraduates_receiving_fed_loans"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true" />

                        <TextView
                            android:id="@+id/aid_fed_loans_value_text_view"
                            android:textColor="@color/colorPrimary"
                            android:text="@string/default_value_of_number_on_college"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@id/aid_fed_loans_label_text_view"
                            android:gravity="right"/>

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="10dp">

                        <TextView
                            android:id="@+id/aid_pell_grants_label_text_view"
                            android:textColor="@color/colorPrimary"
                            android:text="@string/label_pell_grants_recipients"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true" />

                        <TextView
                            android:id="@+id/aid_pell_grants_value_text_view"
                            android:textColor="@color/colorPrimary"
                            android:text="@string/default_value_of_number_on_college"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@id/aid_pell_grants_label_text_view"
                            android:gravity="right"/>

                    </RelativeLayout>
                </LinearLayout>

                <View style="@style/Divider"/>

                <LinearLayout
                    android:orientation="vertical"
                    android:elevation="6dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/colorPrimary"
                        android:textStyle="bold"
                        android:text="@string/label_admission"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="10dp">

                        <TextView
                            android:id="@+id/admission_sat_reading_25_label_text_view"
                            android:text="@string/label_sat_critical_reading_25"
                            android:textColor="@color/colorPrimary"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true" />

                        <TextView
                            android:id="@+id/admission_sat_reading_25_value_text_view"
                            android:text="@string/default_value_of_number_on_college"
